# MongoDB connection settings

[MongoDBFetchURI]
mongodb_uri = mongodb://localhost:27017/

# Common settings for registration, login, forgot password, and entered OTP
[MongoDBFetchCommon]
db_name = register_db
collection_name = register_collection

# Connection settings for sales last 6 months
[MongoDBFetchSalesLast6Months]
db_name = {companyName}
collection_name = {companyName}_salescollection

# Connection settings for sales future 6 months
[MongoDBFetchSalesFuture6Months]
db_name = {companyName}
collection_name = {companyName}_sales_futurecollection

# Connection settings for chargeback last 6 months
[MongoDBFetchChargebackLast6Months]
db_name = {companyName}
collection_name = {companyName}_chargebackcollection

# Connection settings for chargeback future 6 months
[MongoDBFetchChargebackFuture6Months]
db_name = {companyName}
collection_name = {companyName}_chargeback_futurecollection

# Connection settings for sales prediction
[MongoDBFetchSalesprediction]
db_name = {companyName}
collection_name = {companyName}_sales_prediction_collection

# Connection settings for upload sales prediction
[MongoDBUploadSalesprediction]
db_name = {companyName}
collection_name = {companyName}_upload_sales_prediction_collection

# Connection settings for chargeback prediction
[MongoDBFetchChargebackPrediction]
db_name = {companyName}
collection_name = {companyName}_chargeback_prediction_collection

# Connection settings for upload chargeback prediction
[MongoDBUploadChargebackPrediction]
db_name = {companyName}
collection_name = {companyName}_upload_chargeback_prediction_collection

# Connection settings for upload excel data
[MongoDBFetchUploadExcelData]
db_name = {companyName}
collection_name = {companyName}_upload_excel_data_collection

# Connection settings for export data
[MongoDBFetchExportData]
db_name = {companyName}
collection_name = {companyName}_export_data_collection
